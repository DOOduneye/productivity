{
  "name": "server",
  "private": true,
  "main": "build/src/app.js",
  "types": "build/src/app.d.ts",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --env-file=.env src/app.ts",
    "start": "yarn run build && node --env-file=.env build/src/app.js",
    "test": "vitest run /test",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "trunk check ./",
    "lint:fix": "trunk check --fix ./",
    "format": "trunk fmt ./",
    "format:check": "trunk fmt --check ./",
    "clean": "rm -rf build",
    "prepare": "yarn run build",
    "push": "yarn run drizzle-kit push",
    "introspect": "yarn run drizzle-kit pull",
    "generate": "yarn run drizzle-kit generate",
    "migrate": "yarn run drizzle-kit migrate",
    "studio": "yarn run drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^2.1.8",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "globals": "^15.14.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "drizzle-orm": "^0.38.2",
    "express": "^4.21.2",
    "postgres": "^3.4.5"
  }
}
